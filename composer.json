{
    "name": "mr-luke/privileges",
    "description": "Laravel package that adds roles & priviledges to your application.",
    "keywords": ["eloquent", "privileges", "roles", "laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Łukasz Sitnicki",
            "email": "lukasz.sitnicki@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^12.0|^11.0|^10.0|^9.0|^8.0",
        "mr-luke/configuration": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "*",
        "mockery/mockery": "*",
        "orchestra/testbench": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Mrluke\\Privileges\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mrluke\\Privileges\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrluke\\Privileges\\PrivilegesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
